<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" text="Siemens S7 Address Creator">
         <VBox.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0">
         <children>
            <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="0.0" minWidth="0.0" spacing="10.0" style="-fx-border-color: gray;">
               <children>
                  <HBox maxHeight="-Infinity" maxWidth="-Infinity" spacing="10.0">
                     <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Memory Area">
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <ChoiceBox fx:id="memoryAreaChoiceBox" prefWidth="100.0" style="-fx-background-color: transparent; -fx-border-color: lightgray;" />
                     </children>
                  </HBox>
                  <HBox maxHeight="-Infinity" maxWidth="-Infinity" spacing="10.0">
                     <children>
                        <Label fx:id="dbNumberLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="DB Number">
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <TextField fx:id="dbTextField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="50.0" style="-fx-background-color: transparent; -fx-border-color: lightgray;">
                           <font>
                              <Font size="13.0" />
                           </font>
                           <padding>
                              <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                           </padding>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox maxHeight="-Infinity" maxWidth="-Infinity" spacing="10.0">
                     <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Data Type">
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <ChoiceBox fx:id="dataTypeChoiceBox" prefWidth="100.0" style="-fx-background-color: transparent; -fx-border-color: lightgray;" />
                     </children>
                  </HBox>
                  <HBox maxHeight="-Infinity" maxWidth="-Infinity" spacing="10.0">
                     <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Offset">
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <TextField fx:id="offsetTextField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="50.0" style="-fx-background-color: transparent; -fx-border-color: lightgray;">
                           <font>
                              <Font size="13.0" />
                           </font>
                           <padding>
                              <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                           </padding>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox maxHeight="-Infinity" maxWidth="-Infinity" spacing="10.0">
                     <children>
                        <Label fx:id="bitOffsetLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Bit Offset">
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <TextField fx:id="bitOffsetTextField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="50.0" style="-fx-background-color: transparent; -fx-border-color: lightgray;">
                           <font>
                              <Font size="13.0" />
                           </font>
                           <padding>
                              <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                           </padding>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox maxHeight="-Infinity" maxWidth="-Infinity" spacing="10.0">
                     <children>
                        <Label fx:id="stringLengthLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="String Length">
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <TextField fx:id="stringLengthTextField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="50.0" style="-fx-background-color: transparent; -fx-border-color: lightgray;">
                           <font>
                              <Font size="13.0" />
                           </font>
                           <padding>
                              <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                           </padding>
                        </TextField>
                     </children>
                  </HBox>
                  <TextField fx:id="convertedAddressTextField" alignment="CENTER" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="200.0" promptText="Converted Address">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
         <VBox.margin>
            <Insets bottom="4.0" left="8.0" right="8.0" />
         </VBox.margin>
      </StackPane>
      <StackPane fx:id="confirmButtonStackPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0">
         <children>
            <Button fx:id="confirmButton" mnemonicParsing="false" style="-fx-border-color: lightgray; -fx-background-color: transparent;" text="Confirm Address">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding></Button>
         </children>
         <VBox.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </StackPane>
   </children>
</VBox>
